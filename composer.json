{
    "name": "danack/injector",
    "homepage": "https://github.com/Danack/Injector",
    "description": "Danack/Injector is a recursive auto-wiring dependency injector.",
    "keywords": ["dependency injection", "dic", "ioc"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Ackroyd",
            "email": "Danack@basereality.com",
            "homepage": "http://www.basereality.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer" :  "3.3.1",
        "phpbench/phpbench": "1.1.1",
        "symfony/phpunit-bridge": "^6.2.0"
    },
    "autoload": {
        "psr-4": {
            "DI\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DI\\Test\\": "test/"
        },
        "files": ["test/fixtures.php"]
    }
}
